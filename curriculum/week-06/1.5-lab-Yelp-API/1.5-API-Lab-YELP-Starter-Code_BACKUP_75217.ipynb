    "import json\n",
    "\n",
    "# read API keys\n",
    "with open('yelp.json') as cred:\n",
    "    creds = json.load(cred)\n",
    "    #print creds\n",
    "    auth = Oauth1Authenticator(**creds)\n",
    "    client_json = Client(auth)"
   ]
