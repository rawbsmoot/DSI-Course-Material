    "import numpy as np\n",
    "pattern = re.compile(r'[0-9]+')\n",
    "np.max(map(lambda x: int(x), pattern.findall(string)))\n",
    "\n",
    "def clean_column_02(my_col):\n",
    "    for i in enumerate(my_col):\n",
    "        string = my_col[i].encode('ascii')\n",
    "        pattern = re.compile(r'[0-9]+')\n",
