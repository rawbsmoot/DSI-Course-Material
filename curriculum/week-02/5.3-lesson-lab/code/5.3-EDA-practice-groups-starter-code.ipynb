    "def clean_column(my_col):\n",
    "    pattern = re.compile(r'[0-9]+')\n",
    "    for i in my_col:\n",
    "        encoded = i.encode('ascii') \n",
    "        return map(lambda x: int(x), pattern.findall(encoded))  \n",
    "\n",
